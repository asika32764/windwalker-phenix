{
    "name": "phoenix",
    "version": "1.0.0",
    "description": "Phoenix is a RAD (Rapid Application Development) package for Windwalker, it provides a set of Model, View and Controllers\r with different functions to quickly build web admin system, and a code generator to help us create application scaffold.",
    "main": "index.js",
    "scripts": {
        "postinstall": "npm run build -- install",
        "build": "cross-env NODE_ENV=development node_modules/gulp/bin/gulp.js --gulpfile=fusionfile.js",
        "watch": "cross-env NODE_ENV=development node_modules/gulp/bin/gulp.js --gulpfile=fusionfile.js --watch=1",
        "watch:reload": "cross-env NODE_ENV=development node_modules/gulp/bin/gulp.js --gulpfile=fusionfile.js --watch=1 --livereload=1",
        "prod": "cross-env NODE_ENV=production node_modules/gulp/bin/gulp.js --gulpfile=fusionfile.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ventoviro/phoenix.git"
    },
    "author": "",
    "bugs": {
        "url": "https://github.com/ventoviro/phoenix/issues"
    },
    "homepage": "https://github.com/ventoviro/phoenix#readme",
    "dependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.1",
        "@babel/preset-env": "^7.2.0",
        "@claviska/jquery-minicolors": "^2.2.6",
        "backbone": "*",
        "bootstrap": "^4.0.0",
        "eonasdan-bootstrap-datetimepicker": "4.*",
        "font-awesome": "4.*",
        "ivia": "*",
        "jquery": "^3.3.1",
        "jquery-depends-on": "1.*",
        "moment": "~>2.19",
        "moment-timezone": "*",
        "punycode": "1.*",
        "requirejs": "*",
        "select2": "^4.0.6-rc.1",
        "silicone": "^1.0.0-beta1",
        "sortablejs": "^1.7.0",
        "sprintf-js": "1.*",
        "underscore": "*",
        "underscore.string": "*",
        "velocity-animate": "^2.0.5",
        "vue": "^2.5.13",
        "vue-resource": "^1.4.0",
        "vue-router": "^3.0.0",
        "vue-strap": "^1.0.0",
        "vue2-animate": "^1.0.0",
        "vuex": "^3.0.0",
        "windwalker-fusion": "^0.1.8"
    }
}
